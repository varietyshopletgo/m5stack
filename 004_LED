from m5stack import *
from m5ui import *
from uiflow import *
from easyIO import *
import time

setScreenColor(0x222222)







while True:
  digitalWrite(5, 1)
  wait(0.5)
  digitalWrite(5, 0)
  wait(0.5)
  wait_ms(2)
